{
  "name": "@workflow/server",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "pretest": "npm --prefix ../../packages/shared run build",
    "test": "jest",
    "lint": "eslint .",
    "prisma:generate": "node prisma/prisma-cli-with-root-env.cjs generate",
    "prisma:migrate": "node prisma/prisma-cli-with-root-env.cjs migrate dev",
    "prisma:deploy": "node prisma/prisma-cli-with-root-env.cjs migrate deploy",
    "prisma:seed": "node prisma/prisma-cli-with-root-env.cjs db seed",
    "clean": "rimraf dist"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@workflow/shared": "*",
    "@aws-sdk/client-sqs": "3.540.0",
    "@prisma/client": "5.18.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^18.19.76",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ts-api-utils": "^1.3.0",
    "jest": "^29.7.0",
    "prisma": "5.18.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3"
  }
}
